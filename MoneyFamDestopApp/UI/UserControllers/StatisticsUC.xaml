<UserControl x:Class="MoneyFamDestopApp.UI.UserControllers.StatisticsUC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MoneyFamDestopApp.UI.UserControllers"
             mc:Ignorable="d" 
             xmlns:charting="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <charting:CartesianChart x:Name="Graf" Series="{Binding SeriesCollection}"  />
    </Grid>
</UserControl>
